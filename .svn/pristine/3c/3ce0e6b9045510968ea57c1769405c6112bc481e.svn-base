package br.edu.ufu.doutorado.pca.util;
/** 
 * Define qual o tipo de ordenação ascendente (ASC) ou descentende (DESC) 
 * @author afviriato 
 */  
public enum SortType {  
	ASC(1), DESC(-1);  

	private int index = 1;  


	private SortType(int index) {  
		this.index = index;  
	}  

	public int getIndex() {  
		return this.index;  
	}  
}  
